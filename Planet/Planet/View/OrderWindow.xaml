                    </ListBox.ItemTemplate>
                </ListBox>
            </StackPanel>
        </ScrollViewer>
 
        <StackPanel Grid.Row="2" Orientation="Horizontal" HorizontalAlignment="center" Margin="0,0,0,0" >
            <Label Content="Order Name:" VerticalAlignment="Center"/>
            <TextBox Text="{Binding Path=OrderName}" Width="200" Height="25" />
            <Button Command="{Binding OrderCommand}" Content="Place Order" Height="Auto"   Style="{DynamicResource Esri_Button}" Margin="20" VerticalAlignment="Center"/>
        </StackPanel>
        
    </Grid>
</controls:ProWindow>
