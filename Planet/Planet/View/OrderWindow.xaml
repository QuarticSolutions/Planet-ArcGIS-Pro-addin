                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="Auto" />

                                    </Grid.ColumnDefinitions>
                                    <Image Grid.Column="0"  Height="64" HorizontalAlignment="Left" Tag="{Binding Path=title}" OverridesDefaultStyle="True"  >
                                        <Image.Style>
                                            <Style TargetType="{x:Type Image}">
                                                <Setter Property="Source" Value="{Binding Path=image}"/>
                                            </Style>
                                        </Image.Style>
                                    </Image>
