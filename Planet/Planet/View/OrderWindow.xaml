            <TextBlock Width="50"  TextWrapping="Wrap" Text="Visual" HorizontalAlignment="Left" Foreground="{DynamicResource Esri_TextControlBrush}" ></TextBlock>
            <TextBlock Width="50"  TextWrapping="Wrap" Text="Basic" HorizontalAlignment="Left" Foreground="{DynamicResource Esri_TextControlBrush}" ></TextBlock>
        </StackPanel>-->
        <StackPanel Margin="20" Grid.Row="0">
