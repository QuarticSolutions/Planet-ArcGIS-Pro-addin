                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                        
                        <CheckBox Width="70" VerticalAlignment="Center" HorizontalAlignment="Right" Margin="10"  IsChecked="{Binding Path=psscene4Bandselall, Mode=TwoWay}"/>
                        <ComboBox ItemsSource="{Binding TempBundles.PlanetScope4}" DisplayMemberPath="Value" SelectedValuePath="Key" SelectedValue="{Binding  Path=PSScene4Bandcurrselect}"   Margin="10,10,10,10" Width="100" />
                    </StackPanel>
                    <ListBox x:Name="PS42"  Visibility="{Binding PSScene4BandVis}"  Grid.Row="1" Width="Auto" MaxHeight="400"  Grid.Column="0" ScrollViewer.HorizontalScrollBarVisibility="Disabled" ItemsSource="{Binding PSScene4Band}" Margin="10,2,0,0" >
                        <ListBox.ItemTemplate x:Uid="asdasd"  >
